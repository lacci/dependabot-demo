{
  "conan": {
    "enabled": true
  },
  "extends": [
    "config:recommended", 
    ":disableDependencyDashboard"
  ],
  "enabledManagers": [
    "conan"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "conan"
      ],
      "enabled": true,
      "automerge": false
    }
  ],
  "vulnerabilityAlerts": {
    "enabled": true
  },
  "osvVulnerabilityAlerts": true,
  "rebaseWhen": "behind-base-branch",
  "labels": [
    "dependencies",
    "conan"
  ]
}
